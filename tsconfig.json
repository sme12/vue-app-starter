{
    "compilerOptions": {
        "allowJs": true,
        "baseUrl": ".",
        "esModuleInterop": true,
        "isolatedModules": true,
        "moduleResolution": "node",
        "noEmit": true,
        "noImplicitAny": false,
        "paths": {
            "@/*": ["./src/*"]
        },
        "strict": true,
        "target": "ESNext"
    }
}
